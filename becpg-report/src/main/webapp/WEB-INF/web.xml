<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>beCPG report Server</display-name>

	<servlet>
		<servlet-name>report</servlet-name>
		<servlet-class>fr.becpg.report.web.ReportServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>template</servlet-name>
		<servlet-class>fr.becpg.report.web.TemplateServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>report</servlet-name>
		<url-pattern>/report</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>template</servlet-name>
		<url-pattern>/template</url-pattern>
	</servlet-mapping>
</web-app>
