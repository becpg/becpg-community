<Query name="53DB9405-6B9C-FAB9-8922-39423BAC5ECB" type="MDX" connection="beCPG" cube="[Analyse des coûts]" catalog="beCPG OLAP Schema" schema="beCPG OLAP Schema">
  <MDX>SELECT
NON EMPTY {Hierarchize({[Measures].[Total des côuts]})} ON COLUMNS,
NON EMPTY Topcount([Produits.Produit par nom].[Nom produit].Members,20,[Measures].[Total des côuts]) ON ROWS
FROM [Analyse des coûts]
WHERE {Hierarchize({{[Type de côut].[Coût MP], [Type de côut].[Coût Emb]}})}</MDX>
</Query>

