<Query name="450DD884-CA64-B742-A992-9F6C519C84C7" type="MDX" connection="beCPG" cube="[Analyse des coûts]" catalog="beCPG OLAP Schema" schema="beCPG OLAP Schema">
  <MDX>SELECT
NON EMPTY {Hierarchize({[Produits.Produit par famille].[Famille].Members})} ON COLUMNS,
NON EMPTY Topcount([Source.Source par nom].[Nom source].Members,10,[Measures].[Total des côuts]) ON ROWS
FROM [Analyse des coûts]
WHERE {Hierarchize({[Type de côut].[Coût MP]})}</MDX>
</Query>

