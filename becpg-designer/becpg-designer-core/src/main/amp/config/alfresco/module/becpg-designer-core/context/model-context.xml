<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>



	<!-- Registration of new models -->
	<bean id="bcpg.designer.dictionaryBootstrap" parent="dictionaryModelBootstrap"
		depends-on="bcpg.dictionaryBootstrap">
		<property name="models">
			<list>
				<value>alfresco/module/${artifactId}/model/m2Model.xml</value>	
				<value>alfresco/module/${artifactId}/model/designerModel.xml</value>		
			</list>
		</property>

		<property name="labels">
			<list>				
				<value>alfresco/module/${artifactId}/model/m2Model</value>
				<value>alfresco/module/${artifactId}/model/designerModel</value>					
			</list>
		</property>
	</bean>
</beans>
