<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN 2.0//EN' 'http://www.springframework.org/dtd/spring-beans-2.0.dtd'>

<beans>
 <bean id="bcpg.designer.dictionaryBootstrap" parent="dictionaryModelBootstrap"
		depends-on="resourceBundles">
		<property name="models">
			<list>
				<value>alfresco/extension/m2Model.xml</value>	
				<value>alfresco/extension/designerModel.xml</value>		
			</list>
		</property>

	</bean>
	
	<bean id="designerInitService" class="fr.becpg.repo.designer.impl.DesignerInitServiceImpl">
		<property name="nodeService" ref="nodeService" />
		<property name="contentService" ref="contentService" />
		<property name="actionService" ref="actionService" />
		<property name="ruleService" ref="ruleService" />
		<property name="mimetypeService" ref="mimetypeService" />
		<property name="searchService" ref="searchService" />
		<property name="transactionService" ref="transactionService" />
	</bean>
	
</beans>
