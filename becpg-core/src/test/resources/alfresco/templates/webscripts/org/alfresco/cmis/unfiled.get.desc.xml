<webscript kind="org.alfresco.cmiskind">
  <shortname>Unfiled Documents (getUnfiled)</shortname>
  <description>Gets all unfiled documents in the Repository.</description>
  <url>/cmis/unfiled</url>

  <format default="atomfeed"/>
  <responses>
    <response type="cmis.atomfeed"/>
  </responses>
  
  <authentication>guest</authentication>
  <transaction allow="readonly"/>
  <family>CMIS</family>
  <lifecycle>public_api</lifecycle>
</webscript>