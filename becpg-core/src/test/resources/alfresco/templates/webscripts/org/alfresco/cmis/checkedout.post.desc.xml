<webscript kind="org.alfresco.cmiskind">
  <shortname>Checkout Document (checkOut)</shortname>
  <description>
  <![CDATA[
  Create a private working copy of the document.
  ]]>
  </description>
  
  <url>/cmis/checkedout</url>

  <format default="atomentry"/>
