<webscript kind="org.alfresco.cmiskind">
  <shortname>Cancel Checkout (cancelCheckout)</shortname>
  <description>
  <![CDATA[
  Reverses the effect of a check-out. Removes the private working copy of the checked-out document, allowing other documents in the version series
  to be checked out agai
  ]]>
  </description>
  
  <!-- by object id -->
  <url>/cmis/pwc/i/{id}</url>
  <url>/cmis/pwc/s/{store}/i/{id}</url>
  <args>
    <arg>
        <shortname>store</shortname>
        <description>the store name</description>
    </arg>
    <arg>
        <shortname>id</shortname>
        <description>the node id of the object</description>
    </arg>
  </args>
  
  <format default="atomentry"/>

  <authentication>user</authentication>
  <family>CMIS</family>
  <lifecycle>public_api</lifecycle>
</webscript>