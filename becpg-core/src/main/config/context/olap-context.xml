<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>

	<bean id="olapService" class="fr.becpg.repo.olap.impl.OlapServiceImpl">
		<property name="olapPassword" value="${becpg.olap.password}"/>
		<property name="olapServerUrl" value="${becpg.olap.url}"/>
		<property name="olapUser" value="${becpg.olap.user}"/>
		<property name="beCPGCacheService" ref="beCPGCacheService"/>
	</bean>

   
   	<!-- Webscript -->
   	<bean id="webscript.fr.becpg.olap.olapChart.get" class="fr.becpg.repo.olap.webscripts.OlapChartWebScript" parent="webscript">
      <property name="olapService" ref="olapService"/>
   	</bean>
   
</beans>