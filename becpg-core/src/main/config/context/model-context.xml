<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<!-- Registration of new models -->
	<bean id="bcpg.dictionaryBootstrap" parent="dictionaryModelBootstrap"
		depends-on="resourceBundles">
		<property name="models">
			<list>
				<value>alfresco/module/${groupId}-${artifactId}/model/reportModel.xml</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/becpgModel.xml</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/qualityModel.xml</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/securityModel.xml</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/ecoModel.xml</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/m2Model.xml</value>	
				<value>alfresco/module/${groupId}-${artifactId}/model/designerModel.xml</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/npdModel.xml</value>							
			</list>
		</property>

		<property name="labels">
			<list>				
				<value>alfresco/module/${groupId}-${artifactId}/model/dataTypeAnalyzers</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/reportModel</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/becpgModel</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/qualityModel</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/securityModel</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/ecoModel</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/npdModel</value>		
				<value>alfresco/module/${groupId}-${artifactId}/model/m2Model</value>
				<value>alfresco/module/${groupId}-${artifactId}/model/designerModel</value>						
			</list>
		</property>
	</bean>
</beans>
