<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<!-- Registration of new models -->
	<bean id="bcpg.dictionaryBootstrap" parent="dictionaryModelBootstrap"
		depends-on="resourceBundles">
		<property name="models">
			<list>
				<value>alfresco/module/${artifactId}/model/reportModel.xml</value>
				<value>alfresco/module/${artifactId}/model/becpgModel.xml</value>
				<value>alfresco/module/${artifactId}/model/securityModel.xml</value>
			</list>
		</property>

		<property name="labels">
			<list>				
				<value>alfresco/module/${artifactId}/model/reportModel</value>
				<value>alfresco/module/${artifactId}/model/becpgModel</value>
				<value>alfresco/module/${artifactId}/model/securityModel</value>
			</list>
		</property>
	</bean>
	
	
	<!-- dynListConstraint -->
	<bean id="dynListConstraint" class="fr.becpg.repo.dictionary.constraint.DynListConstraint">
		<property name="serviceRegistry" ref="ServiceRegistry" />
	</bean>
	
</beans>
