<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<!-- Registration of new models -->
	<bean id="bcpg.dictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="resourceBundles">
		<property name="models">
			<list>
				<value>alfresco/module/${groupId}-${artifactId}/model/becpgModel.xml</value>
			</list>
		</property>
		
		<property name="labels">
            <list>
                <value>alfresco/module/${groupId}-${artifactId}/model/dataTypeAnalyzers</value>
                <value>alfresco/module/${groupId}-${artifactId}/model/becpgModel</value>
            </list>
        </property>
	</bean>
</beans>
