    <sequenceFlow id="flow41" name="" sourceRef="prevActionTask" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow42" name="" sourceRef="exclusivegateway2" targetRef="parallelgateway2"></sequenceFlow>
