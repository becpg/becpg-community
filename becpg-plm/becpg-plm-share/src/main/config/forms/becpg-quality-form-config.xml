					<field id="qa:product" set="product-info" />
					<field id="bcpg:plants" set="product-info" label="Site de fabrication"/>
					<field id="qa:productUseByDate" set="product-info" />
					

					<set id="batch-info" template="/org/alfresco/components/form/2-column-set.ftl" appearance="bordered-panel" label-id="becpg.forms.set.batch-info.label" />
					<field id="qa:batchId" set="batch-info" />
					<field id="qa:orderId" set="batch-info" />
					<field id="bcpg:clients" force="true" set="batch-info" />
					<field id="bcpg:suppliers" force="true" set="batch-info" />
					<field id="qa:batchComment" set="batch-info">
						<control template="/org/alfresco/components/form/controls/textarea.ftl">
							<control-param name="rows">3</control-param>
						</control>
					</field>
					
					<set id="claim-info" template="/org/alfresco/components/form/2-column-set.ftl" appearance="bordered-panel" label-id="becpg.forms.set.claim-info.label" />

					<field id="qa:claimSource"  set="claim-info"/>
