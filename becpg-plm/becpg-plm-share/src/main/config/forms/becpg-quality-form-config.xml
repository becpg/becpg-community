					<field id="qa:claimTradeContact" set="claim-info"/>
					<field id="qa:claimTradeContactCheck" set="claim-info"/>
					<field id="qa:claimTracking" set="claim-info"/>
					<field id="qa:claimDescription" set="claim-info"/>

					<field id="qa:claimType" set="claim-info"/>
