							<control-param name="ds">becpg/autocomplete/linkedvalue/values/System/QualityLists/bcpg:entityLists/claimOrigin_Hierarchy
							</control-param>
							<control-param name="parent">qa_claimOriginHierarchy1
							</control-param>
						</control>
					</field>

					<field id="qa:claimTreatementDate" set="claim-info"/>
					<field id="qa:claimTreatmentActor" set="claim-info"/>
					<field id="qa:claimTreatementDetails" set="claim-info"/>
					<field id="qa:claimTreatementPrevActions" set="claim-info"/>

					<field id="qa:claimResponseDate" set="claim-info"/>
					<field id="qa:claimResponseActor" set="claim-info"/>
					<field id="qa:claimResponseState" set="claim-info"/>
					<field id="qa:claimResponseDetails" set="claim-info"/>
					
					<field id="qa:claimClosingDate" set="claim-info"/>

					<!-- audit -->
					<set id="audit" appearance="bordered-panel" label-id="becpg.forms.set.audit.label" />
					<field id="cm:created" set="audit">
						<control template="/org/alfresco/components/form/controls/daterange.ftl" />
					</field>
					<field id="cm:modified" set="audit">
						<control template="/org/alfresco/components/form/controls/daterange.ftl" />
					</field>

				</appearance>
			</form>

